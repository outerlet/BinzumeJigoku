<?xml version="1.0" encoding="utf-8"?>
<resources>
	<!-- TimerView -->
	<declare-styleable name="TimerView">
		<attr name="period" format="integer" />
	</declare-styleable>

	<!-- ContentsTextView -->
	<declare-styleable name="ContentsTextView">
		<attr name="textSize" format="dimension" />
		<attr name="textColor" format="color" />
		<attr name="rubySize" format="dimension" />
		<attr name="rubyColor" format="color" />
		<attr name="lineSpace" format="dimension" />
		<attr name="sentenceSpace" format="dimension" />
	</declare-styleable>

	<!-- ContentsTitleView -->
	<declare-styleable name="ContentsTitleView">
		<attr name="duration" format="integer" />
		<attr name="titleSize" format="dimension" />
		<attr name="titleColor" format="color" />
	</declare-styleable>

</resources>
